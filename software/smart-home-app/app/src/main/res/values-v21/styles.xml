<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Base application theme. -->
    <color name="custom_statusbar">#0000CD</color>
    <color name="custom_bn">#1E90FF</color>
    <color name="custom_bg">#ffffff</color>
    <color name="custom_actionbar">#0000FF</color>
    <color name="custom_actionbar_text">#ffffff</color>

    <!-- Base application theme. -->
    <style name="Theme.AppTheme" parent="@style/Theme.AppCompat.Light.DarkActionBar" >
        <!-- Customize your theme here. -->
        <item name="colorPrimaryDark">@color/custom_statusbar</item>
        <item name="colorPrimary">@color/custom_actionbar</item>
        <item name="android:windowBackground">@color/custom_bg</item>
    </style>


    <style name="Button">
        <item name="android:background">@drawable/transition</item>
    </style>
</resources>